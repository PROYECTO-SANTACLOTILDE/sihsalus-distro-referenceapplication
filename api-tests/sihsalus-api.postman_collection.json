						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status 200\", function () {\r",
									"  pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"const json = pm.response.json();\r",
									"\r",
									"pm.test(\"Autenticado\", function () {\r",
									"  pm.expect(json.authenticated).to.be.true;\r",
									"});\r",
									"\r",
									"// Guarda el UUID de sesión por si quieres usarlo luego\r",
									"if (json.sessionId) {\r",
									"  pm.environment.set(\"sessionId\", json.sessionId);\r",
									"}\r",
									""
								],
